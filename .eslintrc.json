{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
           ],
           "prefer-regex-literals": "off",
           "no-useless-escape": "off",
           "no-invalid-regexp": "off",
           "import/prefer-default-export": "off"
    }
}
